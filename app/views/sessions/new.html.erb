<h1>Login</h1>

<%= form_with(local: true, url: login_path) do |form| %>
  <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email, id: :session_email %>
  </div>

  <div class="field">
    <%= form.label :password %>
    <%= form.password_field :password, id: :session_password %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>


<%= link_to 'Back', senses_path %>
